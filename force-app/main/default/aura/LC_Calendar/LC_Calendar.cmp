<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global" >
    <ltng:require styles="{!$Resource.FullCalendar  + '/FullCalendarJS/fullcalendar.css'}"
                  scripts="{!join(',',
                           $Resource.FullCalendar  + '/FullCalendarJS/lib/jquery.min.js',
                           $Resource.FullCalendar  + '/FullCalendarJS/lib/moment.min.js',
                           $Resource.FullCalendar  + '/FullCalendarJS/fullcalendar.js'
                           )}"
                  afterScriptsLoaded="{!c.afterScriptsLoaded}" />
    
    <ltng:require styles="/resource/FullCalendarJS/FullCalendarJS/fullcalendar.css"/>
    
    <aura:attribute name='Objectlist' type='Object[]'/>
    <aura:attribute name="buttonstate" type="Boolean" default="false"/>
    
     <lightning:buttonStateful
        labelWhenOff="List View"
        labelWhenOn="Grid View"
        
        state="{!v.buttonstate}"
                               onclick="{!c.handleClick }"/>
     
        <div id="calendar" class="slds-card">
      

    </div>
    <div id="listcalendar" class="slds-card"/>

     
</aura:component>